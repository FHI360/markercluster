{
  "name": "geoshare",
  "version": "1.0.0",
  "description": "View tag filtered Instagram images in real time",
  "engines": {
    "iojs": "1.5.1"
  },
  "main": "app/app.js",
  "scripts": {
    "dev-server": "webpack --config config/webpack/prerender.config.js & webpack-dev-server --config config/webpack/devServer.config.js --progress --colors --hot --inline",
    "dev": "babel-node --ignore 'node_modules','build' config/serverDev",
    "build": "webpack --config config/webpack/production.config.js --progress --profile --colors",
    "prod": "babel-node --ignore 'node_modules','build' config/serverProd",
    "test-react": "mochify babel/polyfill ./test/app/components/* --reporter spec --transform [ babelify --stage 1 ] --extension .jsx",
    "test-other": "mocha ./test/lib ./test/app/actions ./test/app/stores --compilers js:babel/register",
    "test": "npm run test-react && npm run test-other"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/troutowicz/geoshare.git"
  },
  "author": "Tim Routowicz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/troutowicz/geoshare/issues"
  },
  "homepage": "https://github.com/troutowicz/geoshare",
  "dependencies": {
    "alt": "^0.16.5",
    "autoprefixer-loader": "^1.2.0",
    "babel": "^5.4.3",
    "babel-loader": "^5.1.2",
    "config": "^1.13.0",
    "css-loader": "^0.12.1",
    "extract-text-webpack-plugin": "^0.8.0",
    "good": "^6.1.2",
    "good-console": "^5.0.0",
    "hapi-auth-cookie": "^2.2.0",
    "hapi-shutdown": "^0.1.1",
    "html-loader": "^0.3.0",
    "instagram-node": "^0.5.7",
    "iso": "^4.1.0",
    "json5": "^0.4.0",
    "leaflet": "^0.7.3",
    "leaflet.markercluster": "Leaflet/Leaflet.markercluster#v0.4.0-hotfix.1",
    "less": "^2.5.0",
    "less-loader": "^2.2.0",
    "material-ui": "callemall/material-ui",
    "react": "^0.13.3",
    "react-hot-loader": "^1.2.7",
    "react-leaflet": "^0.6.0",
    "react-tap-event-plugin": "^0.1.6",
    "redis": "^0.12.1",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "stats-webpack-plugin": "0.1.0",
    "style-loader": "^0.12.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.9.7"
  },
  "devDependencies": {
    "babelify": "^6.1.1",
    "chai": "^2.3.0",
    "mocha": "^2.2.5",
    "mochify": "^2.8.1",
    "phantomjs": "^1.9.16",
    "react-tools": "^0.13.3",
    "reactify": "^1.1.1",
    "rewire": "^2.3.3",
    "sinon": "^1.14.1",
    "webpack-dev-server": "^1.8.2"
  }
}
